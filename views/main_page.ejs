      setTimeout(function(){$('#logModal').modal();},420);
   }
   function forgot_on_modal()
   {
      setTimeout(function(){$('#forgotModal').modal();},420);
   }
  function val()
  {
      setTimeout(function(){$('#logModal').modal();},420);
  }

