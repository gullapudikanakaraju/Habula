  function on() 
  {
    document.getElementById("overlay").style.display = "block";
  }

  function off()
  {
    document.getElementById("overlay").style.display = "none"; 
  }

  function off_for_password_change()
  {
    document.getElementById("overlay_for_password_change").style.display = "none"; 
  }

  function off_for_password_change_success()
  {
    document.getElementById("overlay_for_password_change_success").style.display = "none"; 
  }

  function off_for_password()
  {
    document.getElementById("overlay_for_password").style.display = "none"; 
  }

  function off_for_response()
  {
    document.getElementById("overlay_for_response").style.display = "none"; 
  }

  function off_for_login_response()
  {
    document.getElementById("overlay_for_login_response").style.display = "none"; 
  }

  function off_for_waiting()
  {
    document.getElementById("overlay_for_waiting").style.display = "none"; 
  }

  function off_duplicate_email()
  {
    document.getElementById("overlay_duplicate_email").style.display = "none"; 
