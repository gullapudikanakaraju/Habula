      function kaju()
      {
        var a= document.getElementById('email_login').value;
        var b= document.getElementById('password_login').value;
        if(a!="" && b!=""){
        $('#logModal').modal('hide');
        setTimeout(function(){document.getElementById('overlay_for_waiting').style.display="block";},200);
        console.log(a);
        console.log(b);
        var xhttp;
        if(window.XMLHttpRequest)
        {
          xhttp= new XMLHttpRequest();
        }
        else
        {
          xhttp= new ActiveXObject("Microsoft.XMLHTTP");
        }
        xhttp.open('post','https://habula1.herokuapp.com/login',true);
        xhttp.setRequestHeader('content-type','application/json');
        xhttp.send(JSON.stringify({email: a, password: b}));
        xhttp.onreadystatechange= function(){
          console.log(this.readyState);
          if(this.readyState==4)
          {
            console.log(this.responseText);
            var y = JSON.parse(this.responseText);
            console.log(y);
            if(document.getElementById("che").checked)
            {
              localStorage.setItem("a",a);
              localStorage.setItem("b",b);
            }
            else
            {  
              localStorage.removeItem('a');
              localStorage.removeItem('b');
            }
            if(y.success == true)
            {
              setTimeout(function(){document.location.href='https://habula1.herokuapp.com/'+y.branch;},800);
            }
            if(y.success == false)
            {
              setTimeout(function(){$('#overlay_for_waiting').click();},400);
              setTimeout(function(){document.getElementById('overlay_for_login_response').style.display="block";},800);
            }
          }
        } 
        } 
        return false;     
      }

